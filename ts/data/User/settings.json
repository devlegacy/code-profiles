{
  "breadcrumbs.enabled": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": "on",
  // Typography
  // https://github.com/tonsky/FiraCode
  // https://github.com/tonsky/FiraCode/wiki/VS-Code-Instructions
  "editor.fontFamily": "'Fira Code Retina', 'Dank Mono'",
  // "editor.fontSize": 18,
  "editor.fontLigatures": true,
  "editor.fontWeight": "450", // Retina !! Only works with FiraCode-VF.ttf installed, see below when using separated font files (the normal case).
  "editor.inlineSuggest.enabled": true,
  "editor.lineHeight": 1.6,
  "editor.minimap.enabled": false,
  "editor.tokenColorCustomizations": {
    "[*Light*]": {
      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#000"
          }
        }
      ]
    },
    "[*Dark*]": {
      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#fff"
          }
        }
      ]
    },
    "textMateRules": []
  },
  "editor.renderWhitespace": "all",
  "editor.rulers": [
    120
  ],
  "editor.smoothScrolling": true,
  "editor.suggestSelection": "first",
  "editor.wordWrap": "on",
  "extensions.autoUpdate": true,
  "explorer.compactFolders": false,
  "explorer.excludeGitIgnore": false,
  "explorer.openEditors.visible": 1,
  // "files.associations": {
  //   ".env*": "dotenv"
  // },
  // "files.autoSave": "afterDelay",
  "files.eol": "\n",
  "files.trimTrailingWhitespace": true,
  "focus.opacity": "0.7",
  "focus.highlightRange": "block",
  "search.useGlobalIgnoreFiles": true,
  "search.useIgnoreFiles": true,
  "terminal.explorerKind": "external",
  "terminal.external.windowsExec": "",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  // "terminal.integrated.detectLocale": "off",
  "terminal.integrated.fontFamily": "'Fira Code Retina', 'Dank Mono'",
  "terminal.integrated.profiles.windows": {
    "Git Bash": {
      "source": "Git Bash",
      "icon": "terminal-bash",
      "path": [
        "C:\\Program Files\\Git\\bin\\bash.exe"
      ],
      "args": [
        "--login"
      ]
    },
    "Windows Terminal": {
      "path": "C:\\Users\\Devlegacy\\AppData\\Local\\Microsoft\\WindowsApps\\wt.exe"
    },
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
      // "args": ["-NoProfile"]
    },
    // "PowerShell": null,
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    }
  },
  "terminal.integrated.tabs.enabled": false,
  "workbench.colorTheme": "Night Owl (No Italics)",
  "workbench.colorCustomizations": {
    "editorBracketHighlight.foreground1": "#58cdff",
    "editorBracketHighlight.foreground2": "#ff9ce3",
    "editorBracketHighlight.foreground3": "#9161ff",
    "editorBracketHighlight.foreground4": "#e7b454",
    "editorBracketHighlight.foreground5": "#49c54f"
  },
  "workbench.editor.labelFormat": "short",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.list.smoothScrolling": true,
  "workbench.list.horizontalScrolling": true,
  "workbench.tree.indent": 20,
  "workbench.settings.editor": "json",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "welcomePage",
