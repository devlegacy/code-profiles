{
  "scopeName": "source.dotenv",
  "patterns": [
    {
      "comment": "Comments",
      "match": "^\\s?(#.*$)\\n",
      "captures": {
        "1": {
          "patterns": [
            {
              "include": "#reminder"
            }
          ]
        }
      }
    },
    {
      "comment": "Entries",
      "match": "(=.+)",
      "captures": {
        "1": {
          "name": "keyword.other.dotenv"
        }
      }
    }
  ]
}
